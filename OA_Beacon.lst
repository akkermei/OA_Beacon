 105:OA_Beacon.c   ****         /* Enabel timer overflow interrupt*/
 106:OA_Beacon.c   ****         TIMSK |= _BV(TOIE0);
 107:OA_Beacon.c   ****         /* Start timing sequence by setting the prescaler */
 108:OA_Beacon.c   ****         TCCR0B |= TMR0_PRESC_V;
